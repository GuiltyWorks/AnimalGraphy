<div class="main users-index">
  <div class="container">
    <% @users.each do |user| %>
      <div class="users-index-item">
        <a href="<%= "/users/#{user.id}" %>" class="user-item">
          <img src="<%= user.image %>">
          <p><%= user.name %></p>
        </a>
      </div>
    <% end %>
    <div class="users-index-item pagenate">
      <%= paginate @users %>
    </div>
  </div>
</div>
